{
  "extends": [
    "standard"
  ],
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "comma-dangle": [
      "error",
      "only-multiline"
    ],
    "react/jsx-indent": [
      "error",
      2,
      {
        "checkAttributes": true,
        "indentLogicalExpressions": true
      }
    ]
  }
}